# Production Environment Variables Example
# Copy this file to .env.production and update with your actual values

# Database
DATABASE_URL="postgresql://user:password@host:5432/ai_go_production?schema=public"

# Redis
REDIS_URL="redis://default:password@redis-host:6379"

# Authentication
JWT_SECRET="your-super-secure-jwt-secret-min-32-chars"
NEXTAUTH_URL="https://your-domain.com"
NEXTAUTH_SECRET="your-nextauth-secret-min-32-chars"

# API Keys (if needed)
OPENAI_API_KEY="sk-..."
ANTHROPIC_API_KEY="sk-ant-..."
GOOGLE_API_KEY="..."

# Analytics
NEXT_PUBLIC_ANALYTICS_URL="/api/analytics"
NEXT_PUBLIC_GA_MEASUREMENT_ID="G-XXXXXXXXXX"

# Monitoring
SENTRY_DSN="https://..."
SENTRY_ORG="your-org"
SENTRY_PROJECT="ai-go"

# Feature Flags
ENABLE_REDIS_CACHE="true"
ENABLE_WEBSOCKET="true"
ENABLE_ANALYTICS="true"

# Performance
NODE_ENV="production"
NEXT_TELEMETRY_DISABLED="1"

# Security
RATE_LIMIT_PER_MINUTE="100"
CORS_ALLOWED_ORIGINS="https://your-domain.com,https://api.your-domain.com"

# External Services
WEBHOOK_SECRET="your-webhook-secret"
SLACK_WEBHOOK_URL="https://hooks.slack.com/services/..."

# CDN
NEXT_PUBLIC_CDN_URL="https://cdn.your-domain.com"
NEXT_PUBLIC_ASSETS_PREFIX="/assets"