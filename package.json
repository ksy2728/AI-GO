{
  "name": "ai-go",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "dev:next": "next dev --turbopack",
    "build": "prisma generate && next build",
    "build:static": "next build && next export",
    "start": "NODE_ENV=production node server.js",
    "start:vercel": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,css,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:push:prod": "prisma db push --schema=prisma/schema.prod.prisma",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy --schema=prisma/schema.prod.prisma",
    "db:seed": "prisma db seed",
    "db:seed:prod": "tsx prisma/seed.ts --schema=prisma/schema.prod.prisma",
    "db:studio": "prisma studio",
    "db:studio:prod": "prisma studio --schema=prisma/schema.prod.prisma",
    "sync:init": "node scripts/init-sync.js",
    "sync:all": "node scripts/sync-all-providers.js",
    "sync:models": "node scripts/sync-all-models.js",
    "sync:github": "node scripts/sync-github.js",
    "sync:scheduler": "node scripts/sync-scheduler.js",
    "postinstall": "prisma generate",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@formatjs/intl-localematcher": "^0.5.9",
    "@google/generative-ai": "^0.24.1",
    "@octokit/rest": "^22.0.0",
    "@prisma/client": "^6.1.0",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tanstack/react-query": "^5.62.11",
    "@tanstack/react-query-devtools": "^5.62.11",
    "@tanstack/react-table": "8.20.5",
    "@tanstack/react-virtual": "3.10.8",
    "@types/node-cron": "^3.0.11",
    "@vercel/kv": "^3.0.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.12",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.460.0",
    "negotiator": "^0.6.4",
    "next": "^15.4.6",
    "next-intl": "^4.3.4",
    "next-themes": "^0.4.4",
    "node-cron": "^4.2.1",
    "openai": "^5.12.2",
    "prisma": "^6.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-swipeable": "^7.0.2",
    "recharts": "^2.15.4",
    "replicate": "^1.0.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "web-vitals": "^5.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^14.3.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^4.10.0",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/negotiator": "^0.6.4",
    "@types/node": "^22.10.5",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "autoprefixer": "^10.4.20",
    "critters": "^0.0.23",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.1.0",
    "eslint-config-prettier": "^10.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.55.0",
    "postcss": "^8.5.3",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
