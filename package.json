{
  "name": "ai-go",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "dev:next": "next dev --turbopack",
    "build": "prisma generate && prisma migrate deploy && next build",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "build:static": "next build && next export",
    "postinstall": "prisma generate",
    "start": "NODE_ENV=production node server.js",
    "start:vercel": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,css,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:push:prod": "prisma db push --schema=prisma/schema.prod.prisma",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy --schema=prisma/schema.prod.prisma",
    "db:seed": "prisma db seed",
    "db:seed:prod": "tsx prisma/seed.ts --schema=prisma/schema.prod.prisma",
    "db:studio": "prisma studio",
    "db:studio:prod": "prisma studio --schema=prisma/schema.prod.prisma",
    "aggregate-data": "tsx scripts/aggregate-data.ts",
    "generate-report": "tsx scripts/generate-report.ts",
    "check-data-quality": "tsx scripts/check-data-quality.ts",
    "test:scraper:openai": "tsx scripts/test-scraper-openai.ts",
    "test:scraper:anthropic": "tsx scripts/test-scraper-anthropic.ts",
    "scraper-health-report": "tsx scripts/scraper-health-report.ts",
    "sync:init": "node scripts/init-sync.js",
    "sync:all": "node scripts/sync-all-providers.js",
    "sync:models": "node scripts/sync-all-models.js",
    "sync:github": "node scripts/sync-github.js",
    "sync:scheduler": "node scripts/sync-scheduler.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:performance": "playwright test tests/performance/",
    "analyze": "ANALYZE=true next build",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "build:analyze": "npm run build && npm run analyze:bundle",
    "perf:audit": "node scripts/performance-audit.js",
    "perf:lighthouse": "playwright test tests/performance/lighthouse.spec.ts",
    "perf:report": "npm run perf:audit -- http://localhost:3000",
    "validate:production": "node scripts/validate-production.js",
    "pre-deploy": "npm run validate:production && npm run build && npm run typecheck",
    "clean:production": "tsx src/services/clean-production-data.ts",
    "sync:aa": "tsx src/services/sync-aa-real-data.ts",
    "sync:aa:cron": "tsx scripts/setup-aa-cron.ts",
    "sync:aa:test": "tsx src/services/sync-aa-real-data.ts --test"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
